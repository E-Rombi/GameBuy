    Connection = DM.FDConnection1
    SQL.Strings = (
      'select'
      '  p.TITULO,'
      '  sum(pi.VALOR_TOTAL) TOTAL'
      ''
      'from pedido_item pi'
      'inner join produto p on p.ID = pi.FK_PRODUTO'
      'GROUP BY 1'
      'ORDER BY 2 desc')
